{
  "name": "pydantic-gateway",
  "version": "0.0.0",
  "private": true,
  "license": "AGPL-3.0",
  "scripts": {
    "typecheck": "tsc --noEmit && cd test && tsc --noEmit",
    "deploy": "wrangler deploy --var GITHUB_SHA:`git rev-parse HEAD`",
    "dev": "wrangler dev",
    "test": "vitest",
    "lint": "",
    "lint-fix": "",
    "limits-local-db": "wrangler d1 execute pydantic-ai-gateway-limits --local --file limits-schema.sql",
    "limits-remote-db": "wrangler d1 execute pydantic-ai-gateway-limits --remote --file limits-schema.sql",
    "wrangler-secret-put": "wrangler secret put",
    "generate-api-key": "node generate_api_key.js",
    "typegen": "wrangler types --strict-vars false --include-runtime false && prettier worker-configuration.d.ts --write"
  },
  "dependencies": {
    "@pydantic/logfire-api": "^0.8.1",
    "@pydantic/logfire-cf-workers": "^0.6.0"
  },
  "_": "@cloudflare/vitest-pool-workers is pinned because of https://github.com/cloudflare/workers-sdk/issues/10506",
  "devDependencies": {
    "@anthropic-ai/vertex-sdk": "^0.13.2",
    "@cloudflare/vitest-pool-workers": "0.8.44",
    "@cloudflare/workers-types": "^4.20250712.0",
    "groq-sdk": "^0.30.0",
    "openai": "^4.70.3",
    "typescript": "^5.5.2",
    "vitest": "~3.2.0",
    "wrangler": "^4.27.0"
  }
}
