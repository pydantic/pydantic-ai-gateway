{
  "name": "pydantic-gateway-deploy",
  "version": "0.0.0",
  "private": true,
  "license": "AGPL-3.0",
  "scripts": {
    "typecheck": "tsc --noEmit && cd test && tsc --noEmit",
    "deploy": "wrangler deploy --var GITHUB_SHA:`git rev-parse HEAD`",
    "dev": "wrangler dev",
    "test": "vitest",
    "limits-local-db": "wrangler d1 execute pydantic-ai-gateway-limits --local --file ../gateway/limits-schema.sql",
    "limits-remote-db": "wrangler d1 execute pydantic-ai-gateway-limits --remote --file ../gateway/limits-schema.sql",
    "wrangler-secret-put": "wrangler secret put",
    "generate-api-key": "node generate_api_key.js",
    "typegen": "wrangler types --strict-vars false --include-runtime false && prettier worker-configuration.d.ts --write"
  },
  "dependencies": {
    "@pydantic/logfire-api": "^0.8.1",
    "@pydantic/logfire-cf-workers": "^0.6.0"
  },
  "devDependencies": {
    "openai": "^4.70.3",
    "wrangler": "^4.27.0"
  }
}
